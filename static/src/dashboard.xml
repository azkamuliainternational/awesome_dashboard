<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="dashboard">

<div class="bg-green-50 pt-4 sm:pt-4 dark:bg-gray-800">
  <div class="flex items-center justify-center space-x-4 dark:bg-gray-900">
    <!-- Select Category -->
    <div class="w-[150px] text-gray-900 dark:text-gray-100">
      <div class="group relative">
        <label for="category-select" class="text-sm font-semibold text-green-700">Pilih Sales</label>
        <select id="category-select" class="text-site focus:border-brand flex w-full items-center rounded border border-solid border-green-700 bg-white px-3 py-1 font-semibold text-green-700 focus:outline-none focus:ring-0 md:text-sm dark:bg-gray-800">
          <option value="all">All (9)</option>
          <option value="full-stack">Full Stack (6)</option>
          <option value="front-end">Front End (1)</option>
          <option value="freelance">Freelance (1)</option>
          <option value="new-stack-project">New Stack Project (1)</option>
        </select>
      </div>
    </div>
    <!-- Select Month -->
    <div class="w-[150px] text-gray-900 dark:text-gray-100">
      <div class="group relative">
        <label for="month-select" class="text-sm font-semibold text-green-700">Pilih Periode</label>
        <select id="select_periode" class="text-site focus:border-brand flex w-full items-center rounded border border-solid border-green-700 bg-white px-3 py-1 font-semibold text-green-700 focus:outline-none focus:ring-0 md:text-sm dark:bg-gray-800">
          <option value="today">Today</option>
          <option value="thismonth">This Month</option>
          <option value="thisyear">This Year</option>
          <option value="custom">Custom</option>
        </select>
      </div>
    </div>

    <!-- Select Month -->
    <div id="select_month" class="w-[150px] text-gray-900 dark:text-gray-100 invisible ">
      <div class="group relative">
        <label for="month-select" class="text-sm font-semibold text-green-700">Pilih Bulan</label>
        <select id="month-select" class="text-site focus:border-brand flex w-full items-center rounded border border-solid border-green-700 bg-white px-3 py-1 font-semibold text-green-700 focus:outline-none focus:ring-0 md:text-sm dark:bg-gray-800">
          <option value="all">All</option>
          <option value="january">January</option>
          <option value="february">February</option>
          <option value="march">March</option>
          <option value="april">April</option>
          <option value="may">May</option>
          <option value="june">June</option>
          <option value="july">July</option>
          <option value="august">August</option>
          <option value="september">September</option>
          <option value="october">October</option>
          <option value="november">November</option>
          <option value="december">December</option>
        </select>
      </div>
    </div>

    <!-- Select Year -->
    <div id="select_year" class="w-[150px] text-gray-900 dark:text-gray-100 invisible" >
      <div class="group relative">
        <label for="year-select" class="text-sm font-semibold text-green-700">Select Year</label>
        <select id="year-select" class="text-site focus:border-brand flex w-full items-center rounded border border-solid border-green-700 bg-white px-3 py-1 font-semibold text-green-700 focus:outline-none focus:ring-0 md:text-sm dark:bg-gray-800">
          <option value="2024">2024</option>
          <option value="2023">2023</option>
          <option value="2022">2022</option>
          <option value="2021">2021</option>
          <option value="2020">2020</option>
          <option value="2019">2019</option>
        </select>
      </div>
    </div>
  </div>

